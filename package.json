{
  "name": "good-parts-challenges",
  "version": "1.0.0",
  "description": "Challenges of 'Good Parts of JavaScript and the Web' Pluralsight course ",
  "main": "index.js",
  "author": "Norbert Annus",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}