{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      "single"
    ],
    "no-unused-variable": [
      "true"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "max-line-length": [
      true,
      140
    ]
  },
  "rulesDirectory": []
}